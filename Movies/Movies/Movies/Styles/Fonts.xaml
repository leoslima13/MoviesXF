<?xml version="1.0" encoding="UTF-8"?>

<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Font Size -->
    <x:Double x:Key="X-Large-Font">28</x:Double>
    <x:Double x:Key="Medium-Font">16</x:Double>
    <x:Double x:Key="Small-Font">14</x:Double>

    <!-- Fonts -->
    <x:String x:Key="GothamMedium">GothamMedium</x:String>
    <x:String x:Key="RiftDemiItalic">RiftItalic</x:String>
    <x:String x:Key="RobotoMedium">RobotoMedium</x:String>
    <x:String x:Key="RobotoRegular">RobotoRegular</x:String>
    <x:String x:Key="FontAwesomeSolid">FaSolid</x:String>
    <x:String x:Key="FontAwesomeRegular">FaRegular</x:String>

</ResourceDictionary>